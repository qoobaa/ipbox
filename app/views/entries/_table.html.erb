<%= render partial: "entries/form", collection: @entries, as: :entry %>

<table class="table table-hover table-sm" id="entries">
  <thead>
    <tr>
      <th>Date</th>
      <th>Description</th>
      <th class="text-center">Project</th>
      <th class="text-center">Invoice</th>
      <th class="text-center">Type</th>
      <th class="text-center">Duration</th>
    </tr>
  </thead>
  <tbody>
    <%= render @entries %>
  </tbody>
</table>
