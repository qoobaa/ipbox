<tr id="invoice-<%= invoice.id %>">
  <td>
    <%= link_to invoice.number, invoice_path(invoice) %>
  </td>
  <td class="text-center">
    <%= invoice.from %> &mdash; <%= invoice.to %>
  </td>
  <td class="text-right">
    <%= invoice.hours %>
  </td>
  <td class="text-right">
    <%= invoice.entries.sum(:hours) %>
  </td>
</tr>
